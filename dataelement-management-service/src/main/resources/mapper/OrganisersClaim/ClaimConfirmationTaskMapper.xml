<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.inspur.dsp.direct.dao.OrganisersClaim.ClaimConfirmationTaskMapper">

    <!-- 结果映射 ConfirmationTask -->
    <resultMap id="ConfirmationTaskMap" type="com.inspur.dsp.direct.dbentity.ConfirmationTask">
        <id column="task_id" property="taskId"/>
        <result column="base_dataelement_dataid" property="baseDataelementDataid"/>
        <result column="tasktype" property="tasktype"/>
        <result column="send_unit_code" property="sendUnitCode"/>
        <result column="send_unit_name" property="sendUnitName"/>
        <result column="send_date" property="sendDate"/>
        <result column="sender_account" property="senderAccount"/>
        <result column="sender_name" property="senderName"/>
        <result column="status" property="status"/>
        <result column="processing_unit_code" property="processingUnitCode"/>
        <result column="processing_unit_name" property="processingUnitName"/>
        <result column="processing_date" property="processingDate"/>
        <result column="processing_result" property="processingResult"/>
        <result column="processing_opinion" property="processingOpinion"/>
        <result column="processor_account" property="processorAccount"/>
        <result column="processor_name" property="processorName"/>
<!--        <result column="approval_date" property="approvalDate"/>-->
<!--        <result column="approval_account" property="approvalAccount"/>-->
<!--        <result column="approval_name" property="approvalName"/>-->
    </resultMap>

    <!-- 更新确认任务 -->
    <insert id="insertById" parameterType="com.inspur.dsp.direct.dbentity.ConfirmationTask">
        INSERT INTO confirmation_task (
        task_id,
        base_dataelement_dataid,
<!--        tasktype,-->
<!--        send_unit_code,-->
<!--        send_unit_name,-->
        send_date,
        sender_account,
        sender_name,
        status,
        processing_unit_code,
        processing_unit_name,
        processing_date,
        processing_result,
        processing_opinion,
        processor_account,
        processor_name
<!--        approval_date,-->
<!--        approval_account,-->
<!--        approval_name-->
        ) VALUES (
        #{taskId},
        #{baseDataelementDataid},
<!--        #{tasktype},-->
<!--        #{sendUnitCode},-->
<!--        #{sendUnitName},-->
        #{sendDate},
        #{senderAccount},
        #{senderName},
        #{status},
        #{processingUnitCode},
        #{processingUnitName},
        #{processingDate},
        #{processingResult},
        #{processingOpinion},
        #{processorAccount},
        #{processorName}
<!--        #{approvalDate},-->
<!--        #{approvalAccount},-->
<!--        #{approvalName}-->
        )
    </insert>

</mapper>