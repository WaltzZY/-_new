<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.inspur.dsp.direct.dao.OrganisersClaim.ClaimDomainDataElementMapper">

    <!-- 结果映射 DomainDataElement -->
    <resultMap id="DomainDataElementMap" type="com.inspur.dsp.direct.dbentity.DomainDataElement">
        <id column="dataid" property="dataid"/>
        <result column="data_element_id" property="dataElementId"/>
        <result column="base_dataelement_dataid" property="baseDataelementDataid"/>
        <result column="source_unit_code" property="sourceUnitCode"/>
        <result column="source_unit_name" property="sourceUnitName"/>
        <result column="collect_unit_code" property="collectUnitCode"/>
        <result column="collect_unit_name" property="collectUnitName"/>
        <result column="status" property="status"/>
        <result column="create_date" property="createDate"/>
        <result column="create_account" property="createAccount"/>
        <result column="last_modify_date" property="lastModifyDate"/>
        <result column="last_modify_account" property="lastModifyAccount"/>
    </resultMap>

    <!-- 根据基准数据元ID查询领域数据元 -->
    <select id="selectDomainDataElementByBaseDataElementDataId" parameterType="string"
            resultMap="DomainDataElementMap">
        SELECT
        dataid,
        data_element_id,
        base_dataelement_dataid,
        source_unit_code,
        source_unit_name,
        create_date,
        create_account,
        last_modify_date,
        last_modify_account
        FROM domain_data_element
        WHERE base_dataelement_dataid = #{dataid}
    </select>

    <!-- 根据数据元ID查询领域数据元 -->
    <select id="getDataElementById" parameterType="string"
            resultMap="DomainDataElementMap">
        SELECT
        dataid,
        data_element_id,
        base_dataelement_dataid,
        source_unit_code,
        source_unit_name,
        create_date,
        create_account,
        last_modify_date,
        last_modify_account
        FROM domain_data_element
        WHERE dataid = #{dataid}
    </select>

</mapper>
